    <test name="Test editElem() 2" method-name="testEditElem2"/>

    <test name="Test getElem() 1" method-name="testGetElem1"/>
    <test name="Test getElem() 2" method-name="testGetElem2"/>

    <test name="Test getSizeRow() 1" method-name="testGetSizeRow1"/>
    <test name="Test getSizeRow() 2" method-name="testGetSizeRow2"/>

    <test name="Test getSizeColumn() 1" method-name="testGetSizeColumn1"/>
