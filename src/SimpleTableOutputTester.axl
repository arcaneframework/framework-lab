    <test name="Test addElemColumn() / addElemSameRow() 1" method-name="testAddColumnSameRow1"/>
    <test name="Test addElemColumn() / addElemSameRow() 2" method-name="testAddColumnSameRow2"/>
    <test name="Test addElemColumn() / addElemSameRow() 3" method-name="testAddColumnSameRow3"/>

    <test name="Test editElemUp/Down/Left/Right() 1" method-name="testEditElemUDLR1"/>
    <test name="Test editElemDown() 1" method-name="testEditElemDown1"/>
    <test name="Test editElemRight() 1" method-name="testEditElemRight1"/>
  </tests> 

  <options>

    <service-instance name="csv-output" type="ISimpleTableOutput">
      <description>Service CSV</description>
    </service-instance>

    <simple name="csvPath" type="string" default="">
      <description>
        Path and name for csv file (example: ./example.csv ).
      </description>
    </simple>

  </options>

</service>
