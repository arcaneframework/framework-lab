<?xml version="1.0" ?>
 <arcane-config code-name="csv">
  <time-loops>
    <time-loop name="csvLoop">
      <title>csv</title>
      <description>Default timeloop for code csv</description>

      <singleton-services>
        <service name="CsvOutput" need="required" />
      </singleton-services>

      <modules>
        <module name="CsvExample" need="required" />
      </modules>

      <entry-points where="init">
        <entry-point name="CsvExample.InitModule" />
      </entry-points>

      <entry-points where="compute-loop">
        <entry-point name="CsvExample.LoopModule" />
      </entry-points>

      <entry-points where="exit">
        <entry-point name="CsvExample.EndModule" />
      </entry-points>
    </time-loop>
  </time-loops>
</arcane-config>
